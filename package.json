{
   "name":        "dnajs-node-jsdom-jasmine-spec",
   "description": "Sample dna.js project running on Node.js and jsdom executing Jasmine test cases",
   "version":     "1.0.0",
   "license":     "MIT",
   "main":        "app.js",
   "repository":  "dnajs/dnajs-node-jsdom-jasmine-spec",
   "homepage":    "https://github.com/dnajs/dnajs-node-jsdom-jasmine-spec",
   "bugs":        "https://github.com/dnajs/dnajs-node-jsdom-jasmine-spec/issues",
   "docs":        "http://dnajs.org/docs",
   "keywords":    ["dna.js", "node", "jsdom", "jasmine", "javascript"],
   "author":      { "name": "Center Key", "url": "http://centerkey.com" },
   "scripts": {
      "pretest": "jshint *.js",
      "test":    "jasmine spec.js"
   },
   "jshintConfig": {
      "esversion": 6,
      "strict":    "implied",
      "node":      true,
      "jasmine":   true,
      "undef":     true,
      "unused":    true
   },
   "dependencies": {
      "dna.js": "~1.2"
   },
   "devDependencies": {
      "jasmine":               "~2.6",
      "jasmine-spec-reporter": "~4.1",
      "jsdom":                 "~11.0",
      "jshint":                "~2.9"
   }
}
