{
   "name": "dnajs-node-jsdom-jasmine-spec",
   "description": "Sample dna.js project running on Node.js and jsdom executing Jasmine test cases",
   "version": "1.0.0",
   "license": "MIT",
   "main": "app.js",
   "repository": "github:dnajs/dnajs-node-jsdom-jasmine-spec",
   "homepage": "https://github.com/dnajs/dnajs-node-jsdom-jasmine-spec",
   "bugs": "https://github.com/dnajs/dnajs-node-jsdom-jasmine-spec/issues",
   "docs": "https://dnajs.org/docs",
   "author": "Center Key (https://centerkey.com)",
   "keywords": [
      "dna.js",
      "node",
      "jsdom",
      "jasmine",
      "javascript"
   ],
   "jshintConfig": {
      "esversion": 9,
      "strict": "implied",
      "eqeqeq": true,
      "undef": true,
      "unused": true,
      "node": true,
      "jasmine": true
   },
   "scripts": {
      "pretest": "jshint . --exclude node_modules,build,dist",
      "test": "jasmine"
   },
   "dependencies": {
      "dna.js": "~1.9",
      "jquery": "~3.6"
   },
   "devDependencies": {
      "canvas": "~2.8",
      "jasmine": "~4.0",
      "jasmine-spec-reporter": "~7.0",
      "jsdom": "~19.0",
      "jshint": "~2.13"
   }
}
