{
   "name": "node-jsdom-jasmine-spec",
   "description": "Sample dna-engine project running on Node.js and jsdom executing Jasmine test cases",
   "version": "1.0.0",
   "license": "MIT",
   "main": "app.js",
   "repository": "github:dna-engine/node-jsdom-jasmine-spec",
   "homepage": "https://github.com/dna-engine/node-jsdom-jasmine-spec",
   "bugs": "https://github.com/dna-engine/node-jsdom-jasmine-spec/issues",
   "docs": "https://dna-engine.org/docs",
   "author": "Center Key (https://centerkey.com)",
   "keywords": [
      "dna-engine",
      "node",
      "jsdom",
      "jasmine",
      "javascript"
   ],
   "jshintConfig": {
      "esversion": 9,
      "strict": "implied",
      "eqeqeq": true,
      "undef": true,
      "unused": true,
      "node": true,
      "jasmine": true
   },
   "scripts": {
      "pretest": "jshint . --exclude-path .gitignore",
      "test": "jasmine"
   },
   "dependencies": {
      "dna-engine": "~2.2",
      "jquery": "~3.6"
   },
   "devDependencies": {
      "canvas": "~2.10",
      "jasmine": "~4.4",
      "jasmine-spec-reporter": "~7.0",
      "jsdom": "~20.0",
      "jshint": "~2.13"
   }
}
