{
   "name": "dnajs-node-jsdom-jasmine-spec",
   "description": "Sample dna.js project running on Node.js and jsdom executing Jasmine test cases",
   "version": "1.0.0",
   "license": "MIT",
   "main": "app.js",
   "repository": "github:dnajs/dnajs-node-jsdom-jasmine-spec",
   "homepage": "https://github.com/dnajs/dnajs-node-jsdom-jasmine-spec",
   "bugs": "https://github.com/dnajs/dnajs-node-jsdom-jasmine-spec/issues",
   "docs": "https://dnajs.org/docs",
   "keywords": [
      "dna.js",
      "node",
      "jsdom",
      "jasmine",
      "javascript"
   ],
   "author": {
      "name": "Center Key",
      "url": "https://centerkey.com"
   },
   "jshintConfig": {
      "esversion": 6,
      "strict": "implied",
      "undef": true,
      "unused": true,
      "node": true,
      "jasmine": true
   },
   "scripts": {
      "pretest": "jshint *.js spec/*.js spec/**/*.js",
      "test": "jasmine"
   },
   "dependencies": {
      "dna.js": "~1.4",
      "jquery": "~3.3"
   },
   "devDependencies": {
      "jasmine": "~3.1",
      "jasmine-console-reporter": "~3.0",
      "jasmine-spec-reporter": "~4.2",
      "jsdom": "~11.12",
      "jshint": "~2.9"
   }
}
